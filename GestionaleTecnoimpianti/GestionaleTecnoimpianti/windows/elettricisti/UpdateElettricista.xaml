<Window x:Class="GestionaleTecnoimpianti.windows.elettricisti.UpdateElettricista"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GestionaleTecnoimpianti.windows.elettricisti"
        Title="Aggiungi Data Fine" Height="200" Width="295"
        WindowStyle="ToolWindow">
    <Grid>
        <Label Content="Codice Fiscale" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="90"/>
        <Label x:Name="CF_Elettricista" Content="" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="150,10,0,0" Width="125"/>
        <Label Content="Data Fine" HorizontalAlignment="Left" Margin="10,47,0,0" VerticalAlignment="Top" Width="90"/>
        <DatePicker x:Name="Date" HorizontalAlignment="Left" Margin="10,78,0,0" VerticalAlignment="Top" Height="25" Width="265"/>
        <Button Content="Annulla" HorizontalAlignment="Left" Margin="10,129,0,0" VerticalAlignment="Top" Width="125" Background="#FFFF7676" Click="Annulla_Click" />
        <Button Content="Aggiorna" HorizontalAlignment="Left" Margin="150,129,0,0" VerticalAlignment="Top" Width="125" Background="#FF9BFF79" Click="Aggiorna_Click" />
        </Grid>
</Window>
