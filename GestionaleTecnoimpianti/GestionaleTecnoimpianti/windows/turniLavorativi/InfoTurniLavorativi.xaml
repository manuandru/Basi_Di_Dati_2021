        <Label Content="Nota" HorizontalAlignment="Left" Margin="10,226,0,0" VerticalAlignment="Top" Width="150"/>
        <TextBox x:Name="Nota" HorizontalAlignment="Left" Height="51" Margin="10,256,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="319" IsEnabled="False"/>
