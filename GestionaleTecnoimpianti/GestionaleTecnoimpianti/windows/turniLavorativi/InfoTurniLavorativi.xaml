        <Label x:Name="CodImpianto" HorizontalAlignment="Left" Margin="160,41,0,0" VerticalAlignment="Top" Width="169" Height="26"/>
        <Label Content="Data Lavoro" HorizontalAlignment="Left" Margin="10,72,0,0" VerticalAlignment="Top" Width="150"/>
        <Label x:Name="DataLavoro" HorizontalAlignment="Left" Margin="160,72,0,0" VerticalAlignment="Top" Width="169" Height="26"/>
